
config rule 'https'
	option sticky '1'
	option proto 'tcp'
	option dest_port '80,443'
	option use_policy 'Only2'
	option dest_ip '0.0.0.0/0'

config rule '2MainBak'
	option dest_port '0:1023'
	option proto 'all'
	option sticky '1'
	option timeout '600'
	option use_policy '2Main1Bak'
	option dest_ip '0.0.0.0/0'

config rule 'balance'
	option proto 'all'
	option sticky '0'
	option use_policy 'balanced'
	option dest_ip '0.0.0.0/0'
	option dest_port '1024:65535'
	option src_ip '0.0.0.0/0'

config rule 'default_rule'
	option dest_ip '0.0.0.0/0'
	option proto 'all'
	option sticky '0'
	option use_policy 'default'

config globals 'globals'
	option mmx_mask '0x3F00'
	option local_source 'lan'

config policy 'balanced'
	option last_resort 'default'
	list use_member '2WAN_m1w3'
	list use_member '1WAN_m1w3'

config interface '2WAN'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option size '56'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option down '3'
	option up '3'
	option flush_conntrack 'never'
	list track_ip '114.114.114.114'
	list track_ip '119.29.29.29'
	list track_ip '1.1.1.1'
	list track_ip '114.114.115.115'
	option reliability '2'
	option count '3'
	option keep_failure_interval '1'
	option recovery_interval '10'

config interface '1WAN'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option size '56'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option down '3'
	option up '3'
	option flush_conntrack 'never'
	list track_ip '114.114.114.114'
	list track_ip '119.29.29.29'
	list track_ip '114.114.115.115'
	list track_ip '1.1.1.1'
	option reliability '2'
	option count '3'
	option keep_failure_interval '1'
	option recovery_interval '10'

config member '1WAN_m2w3'
	option interface '1WAN'
	option metric '2'
	option weight '3'

config policy 'Only2'
	option last_resort 'default'
	list use_member '2WAN_m1w3'

config member '1WAN_m1w3'
	option interface '1WAN'
	option metric '1'
	option weight '3'

config member '2WAN_m1w3'
	option interface '2WAN'
	option metric '1'
	option weight '3'

config member '2WAN_m1w30'
	option interface '2WAN'
	option metric '1'
	option weight '30'

config policy '2Main1Bak'
	list use_member '2WAN_m1w30'
	list use_member '1WAN_m2w3'
	option last_resort 'default'

